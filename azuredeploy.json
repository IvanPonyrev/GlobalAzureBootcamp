{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "_deploymentsLocationSasToken": {
            "type": "securestring",
            "metadata": {
                "description": "Sas token to deployments container."
            },
            "defaultValue": null
        },
        "_resourcesLocationSasToken": {
            "type": "securestring",
            "metadata": {
                "description": "Sas token to resources container."
            },
            "defaultValue": null
        },
        "_paramsLocationSasToken": {
            "type": "securestring",
            "metadata": {
                "description": "Sas token to params container."
            },
            "defaultValue": null
        },
        "vaultAdminId": {
            "type": "securestring",
            "metadata": {
                "description": "The object id to which vault admin access policies will be applied."
            }
        },
        "secrets": {
            "type": "array",
            "metadata": {
                "description": "An array containing name value pairs."
            },
            "defaultValue": [ ]
        }
    },
    "variables": {
        "deployments": {
            "apiVersion": "[providers('Microsoft.Resources', 'deployments').apiVersions[0]]"
        },
        "secrets": [
            {
                "name": "resourcesLocationSasToken",
                "value": "[parameters('_resourcesLocationSasToken')]"
            },
            {
                "name": "paramsLocationSasToken",
                "value": "[parameters('_paramsLocationSasToken')]"
            },
            {
                "name": "deploymentsLocationSasToken",
                "value": "[parameters('_deploymentsLocationSasToken')]"
            }
        ]
    },
    "resources": [
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "[variables('deployments').apiVersion]",
            "name": "vaults",
            "resourceGroup": "management",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[concat(reference(resourceId(resourceGroup().name, 'Microsoft.Resources/deployments', 'storageAccounts'), variables('deployments').apiVersion).outputs.blobEndpoint.value, 'deployments/vault.json', parameters('_deploymentsLocationSasToken'))]"
                },
                "parameters": {
                    "secrets": {
                        "value": "[union(parameters('secrets'), variables('secrets'))]"
                    },
                    "_resourcesLocationSasToken": {
                        "value": "[parameters('_resourcesLocationSasToken')]"
                    },
                    "vaultAdminId": {
                        "value": "[parameters('vaultAdminId')]"
                    }
                },
                "debugSetting": {
                    "detailLevel": "requestContent, responseContent"
                }
            }
        },
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "[variables('deployments').apiVersion]",
            "name": "automationAccounts",
            "resourceGroup": "management",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[concat(reference(resourceId(resourceGroup().name, 'Microsoft.Resources/deployments', 'storageAccounts'), variables('deployments').apiVersion).outputs.blobEndpoint.value, 'deployments/vault.json', parameters('_deploymentsLocationSasToken'))]"
                },
                "parameters": {
                    "secrets": {
                        "value": "[union(parameters('secrets'), variables('secrets'))]"
                    },
                    "_resourcesLocationSasToken": {
                        "value": "[parameters('_resourcesLocationSasToken')]"
                    },
                    "vaultAdminId": {
                        "value": "[parameters('vaultAdminId')]"
                    }
                },
                "debugSetting": {
                    "detailLevel": "requestContent, responseContent"
                }
            }
        }
    ],
    "outputs": {
        "apiVersion": {
            "type": "string",
            "value": "[variables('deployments').apiVersion]"
        }
    }
}