{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "_resourcesLocationSasToken": {
            "type": "securestring"
        },
        "_paramsLocationSasToken": {
            "type": "securestring"
        },
        "userObjectId": {
            "type": "string"
        }
    },
    "variables": {
      "deployments": {
        "apiVersion": "[providers('Microsoft.Resources', 'deployments').apiVersions[0]]"
      }
    },
    "resources": [
        {
          "type": "Microsoft.Resources/deployments",
          "apiVersion": "[variables('deployments').apiVersion]",
          "name": "vaults",
          "resourceGroup": "vaults",
          "properties": {
            "mode": "Incremental",
            "templateLink": {
              "uri": "[concat(reference(resourceId(resourceGroup().name, 'Microsoft.Resources/deployments', 'storageAccounts'), variables('deployments').apiVersion).outputs.blobEndpoints.value[0], 'resources/vaults.json', parameters('_resourcesLocationSasToken'))]"
            },
            "parameters": {
                "vault": {
                    "value": {
                        "name": "vault",
                        "properties": {
                            "accessPolicies": [
                                {
                                    "objectId": "",
                                    "tenantId": "",
                                    "permissions": {
                                        "keys": [
                                        "all"
                                        ],
                                        "secrets": [
                                        "all"
                                        ],
                                        "certificates": [
                                        "all"
                                        ]
                                    }
                                }
                            ]
                        }
                    }
                },
                "secrets": {
                    "value": [
                        {
                            "name": "_resourcesLocationSasToken",
                            "value": "[parameters('_resourcesLocationSasToken')]"
                        }
                    ]
                },
                "userObjectId": {
                    "value": "[parameters('userObjectId')]"
                }
            },
            "debugSetting": {
              "detailLevel": "requestContent, responseContent"
            }
          }   
        },        
        {
          "type": "Microsoft.Resources/deployments",
          "apiVersion": "[variables('deployments').apiVersion]",
          "name": "networkSecurityGroups",
          "resourceGroup": "network",
          "properties": {
            "mode": "Incremental",
            "templateLink": {
              "uri": "[concat(reference(resourceId(resourceGroup().name, 'Microsoft.Resources/deployments', 'storageAccounts'), variables('deployments').apiVersion).outputs.blobEndpoints.value[0], 'resources/networkSecurityGroups.json', parameters('_resourcesLocationSasToken'))]"
            },
            "parametersLink": {
              "uri": "[concat(reference(resourceId(resourceGroup().name, 'Microsoft.Resources/deployments', 'storageAccounts'), variables('deployments').apiVersion).outputs.blobEndpoints.value[0], 'params/networkSecurityGroups.parameters.json', parameters('_paramsLocationSasToken'))]"
            },
            "debugSetting": {
              "detailLevel": "requestContent, responseContent"
            }
          }   
        },
        {
          "type": "Microsoft.Resources/deployments",
          "apiVersion": "[variables('deployments').apiVersion]",
          "name": "network",
          "resourceGroup": "network",
          "dependsOn": [
              "[resourceId('network', 'Microsoft.Resources/deployments', 'networkSecurityGroups')]"
          ],
          "properties": {
            "mode": "Incremental",
            "templateLink": {
              "uri": "[concat(reference(resourceId(resourceGroup().name, 'Microsoft.Resources/deployments', 'storageAccounts'), variables('deployments').apiVersion).outputs.blobEndpoints.value[0], 'resources/virtualNetworks.json', parameters('_resourcesLocationSasToken'))]"
            },
            "parametersLink": {
              "uri": "[concat(reference(resourceId(resourceGroup().name, 'Microsoft.Resources/deployments', 'storageAccounts'), variables('deployments').apiVersion).outputs.blobEndpoints.value[0], 'params/virtualNetworks.parameters.json', parameters('_paramsLocationSasToken'))]"
            },
            "debugSetting": {
              "detailLevel": "requestContent, responseContent"
            }
          }   
        },
        {
          "type": "Microsoft.Resources/deployments",
          "apiVersion": "[variables('deployments').apiVersion]",
          "name": "subnets",
          "resourceGroup": "network",
          "dependsOn": [
              "[resourceId('network', 'Microsoft.Resources/deployments', 'networkSecurityGroups')]"
          ],
          "properties": {
            "mode": "Incremental",
            "templateLink": {
              "uri": "[concat(reference(resourceId(resourceGroup().name, 'Microsoft.Resources/deployments', 'storageAccounts'), variables('deployments').apiVersion).outputs.blobEndpoints.value[0], 'resources/subnets.json', parameters('_resourcesLocationSasToken'))]"
            },
            "parametersLink": {
              "uri": "[concat(reference(resourceId(resourceGroup().name, 'Microsoft.Resources/deployments', 'storageAccounts'), variables('deployments').apiVersion).outputs.blobEndpoints.value[0], 'params/subnets.parameters.json', parameters('_paramsLocationSasToken'))]"
            },
            "debugSetting": {
              "detailLevel": "requestContent, responseContent"
            }
          }   
        }
    ],
    "outputs": {
      "apiVersion": {
        "type": "string",
        "value": "[variables('deployments').apiVersion]"
      }
    }
}
  