{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "name": {
      "type": "string",
      "metadata": {
        "description": "The name of this resource."
      }
    },
    "apiVersion": {
      "type": "string",
      "metadata": {
        "description": "The apiVersion of this resource. Value 'latest' will deploy using latest apiVersion."
      },
      "defaultValue": "latest"
    }
  },
  "variables": {
    "virtualNetworks": {
      "apiVersion": "[if(equals(parameters('apiVersion'), 'latest'), providers('Microsoft.Network', 'virtualNetworks').apiVersions[0], parameters('apiVersion'))]"
    }
  },
  "resources": [
    {
      "type": "Microsoft.Network/networkSecurityGroups",
      "apiVersion": "[variables('virtualNetworks').apiVersion]",
      "name": "[parameters('networkSecurityGroups')[copyIndex()].name]",
      "location": "[resourceGroup().location]",
      "properties": {
        
      }
    }
  ],
  "outputs": {
    "name": {
      "type": "string",
      "value": "[parameters('name')]"
    },
    "resourceId": {
      "type": "string",
      "value": "[resourceId('Microsoft.Network/networkSecurityGroups', parameters('name'))]"
    }
  }
}