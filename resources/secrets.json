{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "name": {
        "type": "string",
        "metadata": {
          "description": "Secret name."
        }
      },
      "apiVersion": {
        "type": "string",
        "metadata": {
          "description": "The apiVersion of this resource. Value 'latest' will deploy using latest apiVersion."
        },
        "defaultValue": "[providers('Microsoft.KeyVault', 'vaults').apiVersions[0]]"
      },
      "vaultName": {
        "type": "string",
        "metadata": {
          "description": "Parent vault name."
        }
      },
      "value": {
        "type": "securestring",
        "metadata": {
          "description": "The value of the secret. NOTE: 'value' will never be returned from the service, as APIs using this model are intended for internal use in ARM deployments. Users should use the data-plane REST service for interaction with vault secrets."
        }
      }
    },
    "variables": {
    },
    "resources": [
      {
        "type": "Microsoft.KeyVault/vaults/secrets",
        "apiVersion": "[parameters('apiVersion')]",
        "name": "[concat(parameters('vaultName'), '/', parameters('name'))]",
        "location": "[resourceGroup().location]",
        "properties": {
          "value": "[parameters('value')]"
        }
      }
    ],
    "outputs": {
      "name": {
        "type": "string",
        "value": "[parameters('name')]"
      },
      "resourceId": {
        "type": "string",
        "value": "[resourceId('Microsoft.KeyVault/vaults/secrets', parameters('vaultName'), parameters('name'))]"
      }
    }
  }
  