{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "siteName": {
      "type": "string",
      "metadata": {
        "description": "The name to be given to this web."
      }
    },
    "apiVersion": {
      "type": "string",
      "metadata": {
        "description": "The apiVersion of this resource. "
      },
      "defaultValue": "[providers('Microsoft.Web', 'sites').apiVersions[0]]"
    },
    "location": {
      "type": "string",
      "metadata": {
        "description": "Resource location."
      },
      "defaultValue": "[resourceGroup().location]"
    },
    "serverFarmId": {
      "type": "string", 
      "metadata": {
        "description": "The id of this sites serverfarm."
      }
    },
    "netFrameworkVersion": {
      "type": "string", 
      "metadata": {
        "description": "The id of this sites serverfarm."
      },
      "defaultValue": "v4.7"
    },
    "pythonVersion": {
      "type": "string", 
      "metadata": {
        "description": "The id of this sites serverfarm."
      },
      "defaultValue": ""
    },
    "connectionStrings": {
      "type": "array", 
      "metadata": {
        "description": "The id of this sites serverfarm."
      },
      "defaultValue": [ ]
    }
  },
  "variables": {},
  "resources": [
    {
      "type": "Microsoft.Web/sites",
      "apiVersion": "[parameters('apiVersion')]",
      "name": "[parameters('siteName')]",
      "location": "[parameters('location')]",
      "properties": {
        "serverFarmId": "[parameters('serverFarmId')]",
        "netFrameworkVersion": "[parameters('netFrameworkVersion')]",
        "connectionStrings": "[parameters('connectionStrings')]"
      }
    }
  ],
  "outputs": {
    "name": {
      "type": "string",
      "value": "[parameters('siteName')]"
    },
    "apiVersion": {
      "type": "string",
      "value": "[parameters('apiVersion')]"
    },
    "resourceId": {
      "type": "string",
      "value": "[resourceId('Microsoft.Web/sites', parameters('siteName'))]"
    }
  }
}