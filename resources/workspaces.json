{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "name": {
        "type": "string",
        "metadata": {
          "description": "Secret name."
        }
      },
      "apiVersion": {
        "type": "string",
        "metadata": {
          "description": "The apiVersion of this resource."
        },
        "defaultValue": "latest"
      },
      "location": {
          "type": "string",
          "metadata": {
              "description": "Resource location."
          },
          "defaultValue": "southcentralus"
      },
      "vaultId": {
        "type": "string",
        "metadata": {
          "description": "Parent vault name."
        },
        "defaultValue": ""
      },
      "storageAccountId": {
        "type": "securestring",
        "metadata": {
          "description": "The value of the secret. NOTE: 'value' will never be returned from the service, as APIs using this model are is intended for internal use in ARM deployments. Users should use the data-plane REST service for interaction with vault secrets."
        }
      }
    },
    "variables": {
      "workspaces": {
        "apiVersion": "[if(equals(parameters('apiVersion'), 'latest'), providers('Microsoft.MachineLearning', 'workspaces').apiVersions[0], parameters('apiVersion'))]"
      }
    },
    "resources": [
      {
        "type": "Microsoft.MachineLearning/workspaces",
        "apiVersion": "[variables('workspaces').apiVersion]",
        "name": "[parameters('name')]",
        "location": "[parameters('location')]",
        "properties": {
          "keyVaultIdentifierId": "[if(empty(parameters('vaultId')), json('null'), parameters('vaultId'))]",
          "userStorageAccountId": "[parameters('storageAccountId')]"
        }
      }
    ],
    "outputs": {
      "name": {
        "type": "string",
        "value": "[parameters('name')]"
      },
      "apiVersion": {
        "type": "string",
        "value": "[variables('workspaces').apiVersion]"
      },
      "resourceId": {
        "type": "string",
        "value": "[resourceId('Microsoft.MachineLearning/workspaces', parameters('name'))]"
      },
      "reference": {
        "type": "object",
        "value": "[reference(resourceId('Microsoft.MachineLearning/workspaces', parameters('name')), variables('workspaces').apiVersion)]"
      }
    }
}
  